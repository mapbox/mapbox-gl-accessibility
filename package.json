{
  "name": "@mapbox/mapbox-gl-accessibility",
  "version": "0.0.0",
  "description": "An accessibility control for Mapbox GL JS",
  "main": "index.js",
  "scripts": {
    "start": "budo example/index.js --serve=example/bundle.js --live",
    "build": "browserify example/index.js > example/bundle.js",
    "test": "eslint index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mapbox/mapbox-gl-accessibility.git"
  },
  "author": "Mapbox",
  "license": "ISC",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "budo": "^11.5.0",
    "eslint": "^4.19.1",
    "mapbox-gl": "^0.44.2"
  },
  "dependencies": {
    "@turf/bbox": "^6.0.1",
    "lodash": "^4.17.10",
    "xtend": "^4.0.1"
  }
}
